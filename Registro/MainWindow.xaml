        <Button x:Name="btnInserisci" Content="Inserisci" HorizontalAlignment="Left" Margin="305,137,0,0" VerticalAlignment="Top" Height="46" Width="76" Click="btnInserisci_Click"/>
        <TextBlock HorizontalAlignment="Left" Margin="150,94,0,0" TextWrapping="Wrap" Text="Nome" VerticalAlignment="Top" Height="21" Width="48"/>
        <TextBlock HorizontalAlignment="Left" Margin="150,160,0,0" TextWrapping="Wrap" Text="Cognome" VerticalAlignment="Top" Height="18" Width="68"/>

    </Grid>
</Window>
